{
  "name": "joel-gamble-dev",
  "version": "5.0.2",
  "description": "Each month in 2020, I explore a small project that must be completed before the end of the month. I aim to create functional projects and designs using HTML, SASS and Javascript.",
  "scripts": {
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "sass": "sass --style=compressed --source-map-urls=relative src/scss/main.scss _site/css/main.css",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "watch:gulp": "gulp watch",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run sass && gulp build && eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Joel Gamble",
    "email": "jjoelgamble@gmail.com",
    "url": "https://joelgamble.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-sass": "^4.1.0",
    "luxon": "^1.27.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.3.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "sass": "^1.34.1"
  }
}
